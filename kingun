					[[ "$OPTERR" == "1" ]] && [[ "${opts:0:1}" != ":" ]] && {
						help_me
					}
					;;
			esac
			;;
		u)
			echo "$B[*] Applying unlocker...$N"
			case "$2" in
				1)
