kmsg "                                          ENABLING CPUIDLE USE DEEPEST STATE...                                                                 "
kmsg "-------------------------------------------------------------------------------------------------------------------------------------------------"

if [[ -e "/sys/devices/system/cpu/cpuidle/use_deepest_state" ]] 
then
write "/sys/devices/system/cpu/cpuidle/use_deepest_state" "1"
fi

kmsg "-------------------------------------------------------------------------------------------------------------------------------------------------"
kmsg "                                          DISABLING KRAIT VOLTAGE BOOST...                                                                       "
kmsg "-------------------------------------------------------------------------------------------------------------------------------------------------"

