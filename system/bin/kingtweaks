do
avail_scheds="$(cat "${queue}scheduler")"
for sched in cfq noop zen anxiety deadline kyber bfq bfq-sq mq-deadline none
do
if [[ "$avail_scheds" == *"$sched"* ]]
then
write "${queue}scheduler" "$sched"
